{
    "name": "@rbxts/logsnag",
    "version": "1.0.0-beta.2",
    "description": "LogSnag SDK for roblox-ts",
    "main": "out/init.lua",
    "license": "MIT",
    "scripts": {
        "build": "rbxtsc",
        "dev": "rbxtsc -w --type game",
        "prepublishOnly": "yarn run build",
        "lint": "yarn eslint --max-warnings 0 ./src",
        "docs": "yarn typedoc"
    },
    "keywords": [
        "roblox-ts",
        "logsnag",
        "sdk"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ClubiNew/rbxts-logsnag"
    },
    "bugs": {
        "url": "https://github.com/ClubiNew/rbxts-logsnag/issues"
    },
    "types": "out/index.d.ts",
    "files": [
        "out",
        "!**/*.spec.*",
        "!**/*.tsbuildinfo"
    ],
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@rbxts/compiler-types": "^2.2.0-types.0",
        "@rbxts/testez": "^0.4.2-ts.0",
        "@rbxts/types": "^1.0.738",
        "@typescript-eslint/eslint-plugin": "^6.18.1",
        "@typescript-eslint/parser": "^6.18.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-roblox-ts": "^0.0.36",
        "prettier": "^3.2.1",
        "rbxts-transform-env": "^2.2.0",
        "roblox-ts": "^2.2.0",
        "typedoc": "^0.25.7",
        "typedoc-material-theme": "^1.0.2",
        "typedoc-plugin-coverage": "^2.2.0",
        "typedoc-plugin-extras": "^3.0.0",
        "typedoc-plugin-merge-modules": "^5.1.0",
        "typedoc-plugin-replace-text": "^3.2.0",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@rbxts/services": "^1.5.1"
    }
}
